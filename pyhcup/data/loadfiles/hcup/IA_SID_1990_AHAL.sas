*  Data Step                  *;                                                
*******************************;                                                
DATA IA_SIDC_1990_AHAL;                                                         
INFILE 'IA_SID_1990_AHAL.ASC' LRECL = 42;                                       
                                                                                
*** Variable attribute ***;                                                     
ATTRIB                                                                          
  HOSPSTCO           LENGTH=5          FORMAT=Z5.                               
  LABEL="Hospital modified FIPS state/county code"                              
                                                                                
  HOSPID             LENGTH=4          FORMAT=Z5.                               
  LABEL="HCUP hospital ID number (SSHHH)"                                       
                                                                                
  DSHOSPID           LENGTH=$13                                                 
  LABEL="I:Data source hospital ID number"                                      
                                                                                
  YEAR               LENGTH=3                                                   
  LABEL="Calendar year"                                                         
                                                                                
  HOSPST             LENGTH=$2                                                  
  LABEL="Hospital state postal code"                                            
                                                                                
  IDNUMBER           LENGTH=$6                                                  
  LABEL="HCUP AHA ID number without the leading 6"                              
                                                                                
  AHAID              LENGTH=$7                                                  
  LABEL="AHA ID number with the leading 6"                                      
  ;                                                                             
                                                                                
                                                                                
*** Input the variables from the ASCII file ***;                                
INPUT                                                                           
      @1      HOSPSTCO            N5PF.                                         
      @6      HOSPID              N5PF.                                         
      @11     DSHOSPID            $CHAR13.                                      
      @24     YEAR                N4PF.                                         
      @28     HOSPST              $CHAR2.                                       
      @30     IDNUMBER            $CHAR6.                                       
      @36     AHAID               $CHAR7.                                       
      ;                                                                         
                                                                                
                                                                                
RUN;
